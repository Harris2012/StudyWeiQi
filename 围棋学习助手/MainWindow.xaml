						MouseEnter="TypeBorder_MouseEnter"
						MouseLeave="TypeBorder_MouseLeave"
						MouseLeftButtonDown="ClearBorder_MouseLeftButtonDown">
					<StackPanel Orientation="Horizontal">
						<Ellipse Fill="LightCyan" />
						<TextBlock>清除</TextBlock>
					</StackPanel>
				</Border>
