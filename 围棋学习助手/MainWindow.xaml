						MouseLeave="TypeBorder_MouseLeave"
						MouseLeftButtonDown="WhiteBorder_MouseLeftButtonDown">
					<StackPanel Orientation="Horizontal">
						<Ellipse Fill="White" />
						<TextBlock>执白</TextBlock>
					</StackPanel>
